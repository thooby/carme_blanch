<% semantic_form_for @asiento do |f| %>
    <%= f.inputs :name => "Basic" do %>
      <%= f.input :fecha %>
      <%= f.input :tipo, :as => :select %>
      %= f.input :categorium, :as => :select %>
      <%= f.input :monto %>     
      <%= f.input :observaciones %>
      <%= form.buttons %>
   <% end %>
<% end %>